
.feedback-layout {
  width: 610rpx;
  padding: 50rpx 40rpx;
  background-color: #2F294C;
  border-radius:40rpx;
  margin-top: 80rpx;
}
.main-header-pic{
  width: 120rpx;
  height: 120rpx;
  border-radius: 10rpx;
}
.main-header-name{
  font-size:32rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(219,214,236,1);
  line-height:28rpx;
  margin: 30rpx 0 0 28rpx;
}

.feedback-code {
  width: 530rpx;
  height: 530rpx;
  background-color: white;
  border-radius: 10rpx;
  margin: 36rpx auto 27rpx;
  padding: 30rpx;
}
.code{
  width: 100%;
  height: 100%;
}
.main-footer{
  margin: 40rpx 0 60rpx;
}
.main-footer-p{
  color: #999999;
  font-size: 28rpx;
  text-align: center;
  line-height: 34rpx;
}
.save-btn{
  width: 605rpx;
  height: 100rpx;
  color: rgba(255,255,255,0.9);
  background: var(--primaryColor);
  border-radius: 50rpx;
  margin: 66rpx auto 50rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
