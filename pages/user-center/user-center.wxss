.user-center-bg {
    background-image: url("data:image/png;base64,");
    background-repeat: no-repeat;
    background-size: contain;
    background-position: right top;
}

.user-center-head{
    width: 100rpx;
    height: 100rpx;
    border: 15rpx solid rgba(51,42,80,1);
    border-radius: 50%;
}

.user-center-name{
    font-size:34rpx;
    font-family:PingFang SC;
    font-weight:500;
    color:rgba(219,214,236,1);
    line-height:28rpx;
    margin-left: 26rpx;
}
.user-center-data-layout{
    width:690rpx;
    background:rgba(41,37,68,1);
    border-radius:20rpx;
    padding: 54rpx 0 48rpx;
    margin-bottom: 70rpx;
}

.user-center-data-item-layout{

}

.user-center-num-layout{
    align-items: baseline;
}
.user-center-large-num{
    font-size:56rpx;
    font-family:DINCondensedC;
    font-weight:400;
    color:rgba(219,214,236,1);
    line-height:56rpx;
}

.user-center-small-text{
    font-size:24rpx;
    font-family:PingFang SC;
    font-weight:400;
    color:rgba(219,214,236,1);
    line-height:24rpx;
    margin-left: 7rpx;
}

.user-center-middle-text{
    font-size:26rpx;
    font-family:PingFang SC;
    font-weight:400;
    color:rgba(130,121,161,1);
    line-height:28rpx;
    margin-top: 21rpx;
}

.user-center-icon{
    width: 28rpx;
    height: 30rpx;
    margin-right: 18rpx;
    margin-left: 34rpx;
}

.user-center-icon-text{
    font-size:32rpx;
    font-family:PingFang SC;
    font-weight:400;
    color:rgba(184,175,214,1);
    line-height:28rpx;
}

.user-center-my-device-layout{
    width:690rpx;
    border:1rpx solid rgba(74,69,106,1);
    border-radius:20rpx;
    padding: 49rpx 0 49rpx 39rpx;
    margin-top: 39rpx;
}

.user-center-my-device-icon{
    width: 80rpx;
    height: 80rpx;
    margin-right: 28rpx;
}

.user-center-my-device-name{
    font-size:34rpx;
    font-family:PingFang SC;
    font-weight:500;
    color:rgba(219,214,236,1);
    line-height:28rpx;
}

.user-center-my-device-connect-status{
    font-size:26rpx;
    font-family:PingFang SC;
    font-weight:400;
    color:rgba(130,121,161,1);
    line-height:28rpx;
    margin-top: 17rpx;
}

.user-center-my-device-setting-layout{
    padding: 17rpx 37rpx;
}

.user-center-my-device-setting-img{
    width: 14rpx;
    height: 32rpx;
}

.user-center-software-version{
    font-size:24rpx;
    font-family:PingFang SC;
    font-weight:400;
    color:rgba(130,121,161,1);
    line-height:28rpx;
    margin-bottom: 54rpx;
    align-self: flex-end;
}

.user-center-add-device-layout{
    width:690rpx;
    border:2rpx dashed rgba(74,69,106,1);
    border-radius:20rpx;
    padding: 35rpx 0 41rpx;
}

.user-center-add-device-icon{
    width: 58rpx;
    height: 58rpx;
    margin-bottom: 17rpx;
}

.user-center-add-device-content{
    font-size:28rpx;
    font-family:PingFang SC;
    font-weight:400;
    color:rgba(219,214,236,1);
    line-height:28rpx;
}
